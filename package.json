{
  "name": "fy-ui",
  "version": "1.0.31",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist es",
    "prepare": "npm run clean && npm run lint && npm run build",
    "build": "npm run build:es && npm run build:less && npm run build:icon",
    "build:es": "cross-env BABEL_ENV=production NODE_ENV=production babel src --out-dir lib",
    "build:less": "lessc src/index.less lib/index.css",
    "build:icon": "cp -r src/style/core/icon/ lib/resource/",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint src --ext '.js,.jsx'",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:es": "eslint"
  },
  "files": [
    "lib",
    "src"
  ],
  "lint-staged": {
    "{src,stories}/**/*.{js,jsx}": [
      "lint-staged:es"
    ],
    "src/**/*.less": "stylelint --syntax less"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uraincay/fyui.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uraincay/fyui/issues"
  },
  "homepage": "https://github.com/uraincay/fyui#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "less": "^2.7.3",
    "lint-staged": "^4.3.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0"
  },
  "dependencies": {
    "array-tree-filter": "^1.0.1",
    "babel-cli": "^6.26.0",
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.2",
    "css-animation": "^1.4.1",
    "dom-closest": "^0.2.0",
    "omit.js": "^1.0.0",
    "prop-types": "^15.6.0",
    "rc-animate": "^2.4.1",
    "rc-cascader": "^0.12.0",
    "rc-checkbox": "^2.0.3",
    "rc-dialog": "~6.5.10",
    "rc-dropdown": "~1.5.0",
    "rc-form": "^2.0.0",
    "rc-menu": "^5.0.10",
    "rc-pagination": "~1.12.4",
    "rc-rate": "^2.3.0",
    "rc-select": "^7.1.0",
    "rc-notification": "~2.0.0",
    "rc-steps": "^2.5.1",
    "rc-table": "~5.6.9",
    "rc-tabs": "^9.1.10",
    "rc-tooltip": "^3.7.0",
    "rc-tree": "^1.7.9",
    "rc-util": "^4.2.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "rc-input-number": "~3.6.0",
    "shallowequal": "^1.0.2",
    "warning": "^3.0.0"
  }
}
